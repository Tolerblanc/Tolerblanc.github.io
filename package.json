{
  "name": "tolerblanc-blog",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "description": "Tolerblanc's Tech Blog - Migrating from Jekyll to Astro",
  "repository": {
    "type": "git",
    "url": "git://github.com/Tolerblanc/Tolerblanc.github.io.git"
  },
  "keywords": [
    "astro",
    "blog",
    "tech-blog",
    "korean"
  ],
  "author": "Tolerblanc",
  "license": "MIT",
  "engines": {
    "node": ">= 18.0.0",
    "pnpm": ">= 8.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && pagefind --site dist",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check",
    "jekyll:dev": "bundle exec jekyll serve",
    "jekyll:build": "bundle exec jekyll build",
    "convert:posts": "tsx scripts/convert-posts.ts",
    "convert:posts:sample": "tsx scripts/convert-posts.ts --sample 5",
    "convert:posts:dry-run": "tsx scripts/convert-posts.ts --dry-run"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/react": "^4.4.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "astro": "^5.14.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "katex": "^0.16.25",
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "gray-matter": "^4.0.3",
    "pagefind": "^1.4.0",
    "playwright": "^1.56.1",
    "shadcn": "^3.5.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
